@import 'variables'
body
  font-family 'Arial', sans-serif
  color #333
  min-height 100%
  margin 0
  display flex
  flex-direction column
  .holder
    flex 1
  .body
    flex 1
    margin-top 72px
  .container
    width calc(100% - 2rem)
    max-width 1100px
  .body, .header-dir, .footer-dir
    display flex
    justify-content center