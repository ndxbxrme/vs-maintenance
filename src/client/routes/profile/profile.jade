.profile
  h1 Profile
  form(name='myForm', ng-submit='myForm.$valid && profile.save()')
    label Display name
    input(type='text', ng-model='profile.item.displayName')
    label Telephone number
    input(type='text', ng-model='profile.item.telephone')
    label Email address
    input(type='email', ng-model='profile.item.local.email')
    div
      input(type='submit', value=' Save ')